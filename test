#include "main.h"
#include <stdio.h>

/**
 * func - test function
 * @num: Number
 *
 * Return: 0
 */
int func(int num, ...)
{
	int i, j;

	va_list list;

	va_start(list, num);
	i = va_arg(list, int);
	j = va_arg(list, int);
	printf("%d\n", num);
	printf("%d\n", i);
	printf("%d\n", j);
	va_end(list);
	return (0);

}


/**
 * main - test function
 *
 * Return: 0
 */
int main(void)
{
	int num = -2, i = 0;

	printf("%3d\n", num);
	while (num < 0)
	{
		i++;
		num /= 10;
	}
	printf("%d\n", i);
}
